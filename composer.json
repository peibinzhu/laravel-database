{
  "name": "peibin/laravel-database",
  "description": "Peibin database component for Laravel.",
  "license": "MIT",
  "keywords": [
    "php",
    "laravel",
    "database"
  ],
  "authors": [
    {
      "name": "Peibin Zhu",
      "email": "zhupeibin1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "laravel/framework": ">=6.0",
    "nikic/php-parser": "^4.1",
    "peibin/laravel-utils": "^1.0",
    "php-di/phpdoc-reader": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "PeibinLaravel\\Database\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PeibinLaravelTest\\Database\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PeibinLaravel\\Database\\DatabaseServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
